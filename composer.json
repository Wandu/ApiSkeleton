{
    "name": "wandu/api-skeleton",
    "description": "Wandu API Skeleton",
    "keywords": [
        "wandu",
        "framework"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wandu\\Api\\": "src/"
        }
    },
    "require": {
        "wandu/framework": "dev-master",
        "lcobucci/jwt": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "scripts": {
        "serve": "php -S 0.0.0.0:8888 -t public",
        "test": [
            "phpunit --colors=always"
        ]
    }
}
